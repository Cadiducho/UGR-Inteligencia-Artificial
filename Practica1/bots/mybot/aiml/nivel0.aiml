    <pattern>COMO * * LA PARTE PRACTICA</pattern>
    <template><srai>COMO CONSIGO SUPERAR LA PARTE PRACTICA</srai></template>
  </category>
  <!-- 2 -->
  <category>
    <pattern>COMO SE EVALUA EN LA CONVOCATORIA ORDINARIA</pattern>
